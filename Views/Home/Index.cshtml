@{
    ViewData["Title"] = "Home Page";
}

<fast-card>
    <h2>@ViewData["Title"]</h2>
    <fast-button id="button" appearance="accent">Click Me</fast-button>
</fast-card>

<fast-anchor href="#">Anchor</fast-anchor>
<div>
    <style>
        /* For example purposes only. App authors needs to define */
        fast-avatar {
            --avatar-fill-accent-primary: #cf4073;
        }
    </style>
    <fast-avatar alt="Annie's profile image" link="#" name="Annie Lennox" shape="circle" fill="accent-primary"></fast-avatar>
</div>  

<fast-combobox multiple>
    <fast-option value="1">Option 1</fast-option>
    <fast-option value="2">Option 2</fast-option>
    <fast-option value="3">Option 3</fast-option>
</fast-combobox>
<fast-text-field placeholder="name"></fast-text-field>